(*----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------*)
(*Local constants*)
VAR CONSTANT
	WAIT : USINT := 0; (*Function step: Wait for starting the Function chain*)
	LINK_DEVICE : USINT := 1; (*Function step: Link file device*)
	UNLINK_DEVICE : USINT := 2; (*Function step: Unlink the File Device from specific path*)
	ERROR : USINT := 3; (*Function step: Jump to this step in every case of Error occuring in the Function chain*)
END_VAR
(*----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------*)
(*Local functions*)
VAR
	DevLink_0 : DevLink; (*Type of FUB DevLink*)
	DevUnlink_0 : DevUnlink; (*Type of FUB DevUnlink*)
END_VAR
(*----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------*)
(*Local variables*)
VAR
	device_param : STRING[200]; (*Connection path of USB device*)
	device_handle : UDINT;
END_VAR
